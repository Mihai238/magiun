@prefix : <http://www.magiun.io/ontologies/ml#> .
@prefix ml: <http://www.magiun.io/ontologies/ml#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.magiun.io/ontologies/ml> .

<http://www.magiun.io/ontologies/ml> rdf:type owl:Ontology .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/ns/shacl#equals
sh:equals rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#maxInclusive
sh:maxInclusive rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#minInclusive
sh:minInclusive rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#or
sh:or rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#path
sh:path rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#pattern
sh:pattern rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#property
sh:property rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/shacl#targetClass
sh:targetClass rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.magiun.io/ontologies/ml#hasColumns
ml:hasColumns rdf:type owl:ObjectProperty ;
              rdfs:range ml:Column .


###  http://www.magiun.io/ontologies/ml#hasValue
ml:hasValue rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain ml:Column ;
            rdfs:range ml:Value .


###  http://www.magiun.io/ontologies/ml#notHasValue
ml:notHasValue rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.magiun.io/ontologies/ml#cardinality
ml:cardinality rdf:type owl:DatatypeProperty ;
               rdfs:domain ml:Column ;
               rdfs:range xsd:integer .


###  http://www.magiun.io/ontologies/ml#value
ml:value rdf:type owl:DatatypeProperty ;
         rdfs:subPropertyOf owl:topDataProperty ;
         rdfs:domain ml:Value .


#################################################################
#    Classes
#################################################################

###  http://www.magiun.io/ontologies/ml#BooleanColumn
ml:BooleanColumn rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( ml:CategoricalColumn
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty ml:hasValue ;
                                                              owl:someValuesFrom ml:BooleanValue
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf ml:CategoricalColumn .


###  http://www.magiun.io/ontologies/ml#BooleanValue
ml:BooleanValue rdf:type owl:Class ;
                rdfs:subClassOf ml:Value ;
                sh:property [ sh:or (
                    [sh:pattern "0"]
                    [sh:pattern "1"]
                )
                ] .


###  http://www.magiun.io/ontologies/ml#CategoricalColumn
ml:CategoricalColumn rdf:type owl:Class ;
                     owl:equivalentClass ml:DiscretizationSuitable ,
                                         [ owl:intersectionOf ( ml:Column
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty ml:cardinality ;
                                                                  owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                       owl:onDatatype xsd:integer ;
                                                                                       owl:withRestrictions ( [ xsd:minInclusive 0
                                                                                                              ]
                                                                                                              [ xsd:maxInclusive 10
                                                                                                              ]
                                                                                                            )
                                                                                     ]
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf ml:Column .


###  http://www.magiun.io/ontologies/ml#Column
ml:Column rdf:type owl:Class .


###  http://www.magiun.io/ontologies/ml#DiscretizationSuitable
ml:DiscretizationSuitable rdf:type owl:Class ;
                          rdfs:subClassOf ml:OperationSuitable .


###  http://www.magiun.io/ontologies/ml#GenderColumn
ml:GenderColumn rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( ml:Column
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty ml:hasValue ;
                                                             owl:someValuesFrom ml:GenderValue
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf ml:CategoricalColumn .


###  http://www.magiun.io/ontologies/ml#GenderValue
ml:GenderValue rdf:type owl:Class ;
               rdfs:subClassOf ml:Value ;
               sh:property [ sh:or (
                                   [sh:pattern "[Mm]"]
                                   [sh:pattern "[Ff]"]
                                   [sh:pattern "[Mm]ale"]
                                   [sh:pattern "[Ff]emale"]
                               )
                               ].


###  http://www.magiun.io/ontologies/ml#HumanAgeColumn
ml:HumanAgeColumn rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( ml:Column
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty ml:hasValue ;
                                                               owl:someValuesFrom ml:HumanAgeValue
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty ml:cardinality ;
                                                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                    owl:onDatatype xsd:integer ;
                                                                                    owl:withRestrictions ( [ xsd:minInclusive 5
                                                                                                           ]
                                                                                                         )
                                                                                  ]
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf ml:Column .


###  http://www.magiun.io/ontologies/ml#HumanAgeValue
ml:HumanAgeValue rdf:type owl:Class ;
                 rdfs:subClassOf ml:Value ;
                 sh:property [ sh:maxInclusive 150 ;
                               sh:minInclusive 0
                             ] .


###  http://www.magiun.io/ontologies/ml#IntValue
ml:IntValue rdf:type owl:Class ;
            rdfs:subClassOf ml:Value .


###  http://www.magiun.io/ontologies/ml#MaritalStatusColumn
ml:MaritalStatusColumn rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( ml:CategoricalColumn
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty ml:hasValue ;
                                                                    owl:someValuesFrom ml:MaritalStatusValue
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf ml:CategoricalColumn .


###  http://www.magiun.io/ontologies/ml#MaritalStatusValue
ml:MaritalStatusValue rdf:type owl:Class ;
                      rdfs:subClassOf ml:Value ;
                      sh:property [ sh:or (
                            [sh:pattern "Widowed"]
                            [sh:pattern "Divorced"]
                            [sh:pattern "Separated"]
                            [sh:pattern ".*[Mm]arried.*"]
                        )
                      ] .


###  http://www.magiun.io/ontologies/ml#OperationSuitable
ml:OperationSuitable rdf:type owl:Class ;
                     rdfs:subClassOf ml:Column .


###  http://www.magiun.io/ontologies/ml#StringValue
ml:StringValue rdf:type owl:Class ;
               rdfs:subClassOf ml:Value .


###  http://www.magiun.io/ontologies/ml#Value
ml:Value rdf:type owl:Class .


###  http://www.w3.org/ns/shacl#NodeShape
sh:NodeShape rdf:type owl:Class .


