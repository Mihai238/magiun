#################################################################

###  http://www.magiun.io/ontologies/ml#FemaleValueShape
ml:FemaleValueShape rdf:type owl:NamedIndividual ,
                             sh:NodeShape ;
                    sh:property [ sh:path ml:value ;
                                  sh:pattern "f"
                                ] ;
                    sh:targetClass ml:FemaleValue .


###  http://www.magiun.io/ontologies/ml#booleanColumn
ml:booleanColumn rdf:type owl:NamedIndividual ;
                 ml:hasValue ml:trueValue ;
                 ml:differentValueTypesCount 2 .


###  http://www.magiun.io/ontologies/ml#falseValue
ml:falseValue rdf:type owl:NamedIndividual ,
                       ml:FalseValue .


###  http://www.magiun.io/ontologies/ml#femaleValue
ml:femaleValue rdf:type owl:NamedIndividual ,
                        ml:FemaleValue ;
               ml:value "f" .


###  http://www.magiun.io/ontologies/ml#genderColumn
ml:genderColumn rdf:type owl:NamedIndividual ;
                ml:hasValue ml:femaleValue ,
                            ml:maleValue ;
                ml:differentValueTypesCount 2 .


###  http://www.magiun.io/ontologies/ml#maleValue
ml:maleValue rdf:type owl:NamedIndividual ,
                      ml:MaleValue ;
             ml:value "m" .


###  http://www.magiun.io/ontologies/ml#trueValue
ml:trueValue rdf:type owl:NamedIndividual ,
                      ml:TrueValue .


